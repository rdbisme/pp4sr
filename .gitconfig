[filter "nbstrip_full"]
clean = "jq --indent 1 \
        '(.cells[] | select(has(\"outputs\")) | .outputs) = []  \
        | (.cells[] | select(has(\"execution_count\")) | .execution_count) = null  \
        | .metadata = {\
        \"language_info\": {\"name\": \"python\", \"pygments_lexer\": \"ipython3\"},\
        \"slideshow\": {\"slide_type\": .metadata.slideshow.slide_type}\
        } \
        | .cells[].metadata = {} \
        '"
smudge = cat
required = true
